        <site name="tm_outer_oblique_right_origin" pos="0.041961 -0.065624 -0.070352" />
      </body>

      <!-- Hyoid bone -->
      <body name="hyoid" childclass="bone" pos="0.0 0.0 0.0" quat="1 0 0 0">
        <!-- Used for visualization and mesh property calculations (intertia, mass, ...) -->
        <geom
          name="hyoid_vis"
          mesh="hyoid_mesh_vis"
        />

        <!-- Hyoid muscle attachment sites -->
        <site name="geniohyoid_left_origin" pos="0.047506 0.001795 -0.146889" />
        <site name="geniohyoid_right_origin" pos="0.047493 -0.001868 -0.147" />
        <site name="anterior_digastric_left_origin" pos="0.043049 0.014645 -0.145468" />
        <site name="anterior_digastric_right_origin" pos="0.043049 -0.014592 -0.145468" />
        <site name="posterior_mylohyoid_left_origin" pos="0.042443 0.014947 -0.14533" />
        <site name="posterior_mylohyoid_right_origin" pos="0.042447 -0.014822 -0.145319" />
        <site name="anterior_mylohyoid_left_origin" pos="0.048461 0.002094 -0.148097" />
        <site name="anterior_mylohyoid_right_origin" pos="0.048461 -0.002041 -0.148097" />
      </body>

      <!-- Movable mandible with TMJ constraints -->
      <body name="mandible" childclass="bone" pos="0 0 0" quat="1 0 0 0">
        <!-- Used for visualization and mesh property calculations (intertia, mass, ...) -->
        <geom
          name="mandible_vis"
          mesh="mandible_mesh_vis"
        />

        <!-- Collision geometry -->
        <attach model="jaw_model_mandible_teeth_coll_geoms" body="mandible_teeth_coll_geoms"
          prefix="jaw_" />

        <!-- TMJ joints -->
        <joint name="tmj_slide_xz" class="tmj_slide"
          range="0.0 0.01" axis="0.006894 0.0 -0.0034014"
          stiffness="1.0e3" damping="63"
          pos="0.0191167 0.0504874 -0.0708849" />

        <joint name="tmj_slide_y" class="tmj_slide"
          range="-0.0075 0.0075" axis="0 1 0"
          stiffness="100.0" damping="18" />

        <joint name="tmj_hinge_x" class="tmj_hinge"
          range="-3 3" axis="1 0 0"
          stiffness="100.0" damping="18"
          pos="0.0191167 0.00001317 -0.0708849" />

        <joint name="tmj_hinge_y" class="tmj_hinge"
          range="-5 18" axis="0 1 0"
          stiffness="5.0" damping="4"
